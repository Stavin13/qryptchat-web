{
	"compilerOptions": {
		"checkJs": false,
		"moduleResolution": "bundler",
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext", "DOM"],
		"paths": {
			"$lib": ["./src/lib"],
			"$lib/*": ["./src/lib/*"]
		}
	},
	"include": ["src/**/*.js", "src/**/*.svelte"],
	"exclude": ["node_modules", ".svelte-kit", "build"]
}
